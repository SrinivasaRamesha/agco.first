{
	"_version": "1.8.0",
	"sap.app": {
		"dataSources": {
			"init_forecast_data": {
				"uri": "model/localData.json",
				"type": "JSON"
			},
			"init_module_data": {
				"uri": "model/moduleDetails.json",
				"type": "JSON"
			},
			"init_vendor_data":{
				"uri":"model/vendorData.json",
				"type":"JSON"
			}
		},
		"id": "incture.forecast.forecasting",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "ui5template.basicSAPUI5ApplicationProject",
			"version": "1.40.12"
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_hcb",
			"sap_belize"
		]
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "incture.forecast.forecasting.view.Main",
			"type": "XML"
		},
		"dependencies": {
			"minUI5Version": "1.30.0",
			"libs": {
				"sap.ui.layout": {},
				"sap.ui.core": {},
				"sap.m": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "incture.forecast.forecasting.i18n.i18n"
				}
			},
			"vendorModel":{
				"type":"sap.ui.model.json.JSONModel",
				"dataSource":"init_vendor_data"
			},

			"modules": {
				"type": "sap.ui.model.json.JSONModel",
				"dataSource": "init_module_data"
			}
		},
		"resources": {
			"css": [{
				"uri": "css/style.css"
			}]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"async": true,
				"viewPath": "incture.forecast.forecasting.view",
				"controlAggregation": "pages",
				"controlId": "idAppControl",
				"clearControlAggregation": false
			},
			"routes": [{
				"name": "RouteMain",
				"pattern": "",
				"target": [
					"TargetMain"
				]
			}, {
				"name": "detail",
				"pattern": "detail/{vendornumber},{vendorname}",
				"target": [
					"detail"
				]
			}],
			"targets": {
				"TargetMain": {
					"viewType": "XML",
					"transition": "slide",
					"clearControlAggregation": false,
					"viewName": "Forecast",
					"routerClass": "sap.m.routing.Router",
					"async": true,
					"viewPath": "incture.forecast.forecasting.view",
					"controlAggregation": "pages",
					"controlId": "idAppControl"
				},
				"detail": {
					"viewType": "XML",
					"viewName": "detail"
				}
			}
		}
	}
}