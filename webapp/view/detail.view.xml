<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:tnt="sap.tnt" xmlns:l="sap.ui.layout"
	xmlns:dat="sap.ui.model.type" controllerName="incture.forecast.forecasting.controller.detail" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:lay="sap.ui.layout.form">
	<Page showHeader="true">
		<customHeader>
			<Toolbar>
				<Button icon="sap-icon://nav-back" press="onBackPress" tooltip="Navigate to previous screen"/>
				<Label text="Vendor Number"/>
				<Text id="vendorPageNumId"/>
				<Label text="Vendor Name"/><Text id="vendorPageNameId"/><Label text="Date"/>
			</Toolbar>
		</customHeader>
		<content>
			<Panel expanded="true" headerText="Vendor Forecast">
				<content>
					<Table id="vendorLevelTabId">
						<columns>
							<Column>
								<header>
									<Label/>
								</header>
							</Column>
							<Column>
								<header>
									<Label text="January"/>
								</header>
							</Column>
							<Column>
								<header>
									<Label text="February"/>
								</header>
							</Column>
							<Column>
								<header>
									<Label text="March"/>
								</header>
							</Column>
							<Column>
								<header>
									<Label text="April"/>
								</header>
							</Column>
							<Column>
								<header>
									<Label text="May"/>
								</header>
							</Column>
							<Column>
								<header>
									<Label text="June"/>
								</header>
							</Column>
							<Column>
								<header>
									<Label text="July"/>
								</header>
							</Column>
							<Column>
								<header>
									<Label text="August"/>
								</header>
							</Column>
							<Column>
								<header>
									<Label text="September"/>
								</header>
							</Column>
							<Column>
								<header>
									<Label text="October"/>
								</header>
							</Column>
							<Column>
								<header>
									<Label text="November"/>
								</header>
							</Column>
							<Column>
								<header>
									<Label text="December"/>
								</header>
							</Column>
						</columns>
						<dependents></dependents>
					</Table>
				</content>
			</Panel>
			<Panel expandable="true" expanded="false" headerText="Parts Forecast" class="sapUiResponsiveMargin">
				<content>
					<FlexBox>
						<List items="{partsModel>/100250}">
							<headerToolbar>
								<Toolbar>
									<Label text="Parts"/>
								</Toolbar>
							</headerToolbar>
							<DisplayListItem type="Navigation" highlight="Information" press="onPartSelection" label="{partsModel>parts}" value="{partsModel>partName}"></DisplayListItem>
						</List>
						<Table headerText="Forecast" id="vendorLevelTabIdSec">
							<columns>
								<Column>
									<header>
										<Label/>
									</header>
								</Column>
								<Column>
									<header>
										<Label text="January"/>
									</header>
								</Column>
								<Column>
									<header>
										<Label text="February"/>
									</header>
								</Column>
								<Column>
									<header>
										<Label text="March"/>
									</header>
								</Column>
								<Column>
									<header>
										<Label text="April"/>
									</header>
								</Column>
								<Column>
									<header>
										<Label text="May"/>
									</header>
								</Column>
								<Column>
									<header>
										<Label text="June"/>
									</header>
								</Column>
								<Column>
									<header>
										<Label text="July"/>
									</header>
								</Column>
								<Column>
									<header>
										<Label text="August"/>
									</header>
								</Column>
								<Column>
									<header>
										<Label text="September"/>
									</header>
								</Column>
								<Column>
									<header>
										<Label text="October"/>
									</header>
								</Column>
								<Column>
									<header>
										<Label text="November"/>
									</header>
								</Column>
								<Column>
									<header>
										<Label text="December"/>
									</header>
								</Column>
							</columns>
							<dependents></dependents>
						</Table>
					</FlexBox>
				</content>
			</Panel>
		</content>
		<footer>
			<Bar>
				<contentRight>
					<Button text="back" press="onBackPress"/>
				</contentRight>
			</Bar>
		</footer>
	</Page>
</mvc:View>